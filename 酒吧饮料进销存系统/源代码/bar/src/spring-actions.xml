		<property name="productDao">
			<ref bean="productDao" />
		</property>
		<property name="jinhuoDao">
			<ref bean="jinhuoDao" />
		</property>
		<property name="kucunDao">
			<ref bean="kucunDao" />
		</property>
		<property name="xiaoshouDao">
			<ref bean="xiaoshouDao" />
		</property>
		<property name="gongyingshangDao">
			<ref bean="gongyingshangDao" />
		</property>
		<property name="xiaoshouitemDao">
			<ref bean="xiaoshouitemDao" />
		</property>

	</bean>





</beans>
